{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "./dist/",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "youtube"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}